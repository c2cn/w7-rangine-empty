{
	"name": "w7/rangine-demo",
	"type": "project",
	"description": "基于 Swoole 的常驻内存型 PHP 高性能框架",
	"license": "Apache-2.0",
	"keywords": [
		"we7",
		"swoole"
	],
	"homepage": "https://www.rangine.com/",
	"authors": [
		{
			"name": "we7team",
			"email": "donknap@gmail.com"
		}
	],
	"autoload": {
		"psr-4": {
			"W7\\App\\": "app/",
			"W7\\Tests\\": "tests/"
		},
		"files": [
			"config/define.php",
			"app/Helper/Functions.php"
		],
		"classmap": [
			"database/seeds",
			"database/migrations"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"W7\\Tests\\" : "tests/"
		}
	},
	"repositories": [
		{
			"type": "path",
			"url": "../swoole"
		},
		{
			"type": "path",
			"url": "../w7-rangine-command"
		},
		{
			"type": "path",
			"url": "../w7-rangine-package-plugin"
		},
		{
			"type": "path",
			"url": "../w7-rangine-crontab"
		},
		{
			"type": "path",
			"url": "../w7-rangine-database-tool"
		}
	],
	"require": {
		"w7/rangine": "dev-master",
		"w7/rangine-command": "dev-master",
		"w7/rangine-package-plugin": "dev-master",
		"w7/rangine-crontab": "dev-master",
		"w7/rangine-database-tool": "dev-master"
	},
	"require-dev": {
		"w7/rangine-debugger": "*",
		"phpunit/phpunit": "^7.0"
	}
}